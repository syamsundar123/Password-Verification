def factorial(n):
    l = [-1 for i in range(n + 1)]
    l[0] = 1

    if n == 0 or n == 1:
        return 1
    for i in range(1, n + 1):
        l[i] = i * l[i - 1]
    return l[n]


def catalan_number(n):


    a = factorial(2 * n)
    b = factorial(n + 1)
    c = factorial(n)

    c_n = a // (b * c)

    return c_n


if __name__ == '__main__':
   
   N = int(input())
   print(catalan_number(N))




